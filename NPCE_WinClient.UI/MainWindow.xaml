<Window x:Class="NPCE_WinClient.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NPCE_WinClient.UI"
        xmlns:view="clr-namespace:NPCE_WinClient.UI.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="300"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <view:NavigationView DataContext="{Binding NavigationViewModel}"></view:NavigationView>
        <!--<view:ServiceDetailView Grid.Column="1" DataContext="{Binding ServiceDetailViewModel}"></view:ServiceDetailView>-->
        <view:AnagraficaDetailView Grid.Column="1" DataContext="{Binding AnagraficaDetailViewModel}"></view:AnagraficaDetailView>
    </Grid>

</Window>
